.custom-tabbar {
  display: flex;
  flex-direction: row;
  background-color: #ffffff;
  height: 100rpx;
  width: 100%;
  justify-content: space-around;
  align-items: center;
  border-top: 1rpx solid #E5E5E5;
}

.tabbar-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex: 1;
  height: 90rpx;
}

.tabbar-label {
  font-size: 22rpx;
  color: #999;
  margin-top: 4rpx;
}

.tabbar-label.label-active {
  color: #FF6B35;
}
