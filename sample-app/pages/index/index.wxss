.container {
  padding: 20rpx;
  background-color: #F5F5F5;
}

.section {
  background-color: #FFFFFF;
  border-radius: 12rpx;
  padding: 24rpx;
  margin-bottom: 24rpx;
}

.section-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  margin-bottom: 24rpx;
}

.row {
  flex-direction: row;
  align-items: center;
  gap: 16rpx;
  flex-wrap: wrap;
  margin-top: 16rpx;
}
